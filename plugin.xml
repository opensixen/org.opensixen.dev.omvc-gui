<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.opensixen.dev.omvc.UpdateMenuAction"
         point="org.opensixen.osgi.interfaces.IMenuAction">
      <client
            class="org.opensixen.dev.omvc.UpdateMenuAction">
      </client>
   </extension>
   <extension
         point="org.opensixen.osgi.interfaces.IMenuAction">
      <client
            class="org.opensixen.dev.omvc.SuscribeMenuAction">
      </client>
   </extension>
   <extension
         point="org.opensixen.osgi.interfaces.IMenuAction">
      <client
            class="org.opensixen.dev.omvc.RunICommandMenuAction">
      </client>
   </extension>
   <extension
         id="OSXServiceConnectionHandler"
         point="org.eclipse.equinox.security.callbackHandler">
      <callbackHandler
            class="org.opensixen.dev.omvc.OSXServiceConnectionHandler">
      </callbackHandler>
   </extension>
   <extension
         point="org.eclipse.equinox.security.callbackHandlerMapping">
      <callbackHandlerMapping
            callbackHandlerId="org.opensixen.dev.omvc-gui.OSXServiceConnectionHandler"
            configName="omvc">
      </callbackHandlerMapping>
   </extension>
   <extension
         id="StartupModelValidator"
         point="org.opensixen.osgi.interfaces.IModelValidator">
      <validator
            class="org.opensixen.dev.omvc.StartupModelValidator">
      </validator>
   </extension>

</plugin>
